<h1>Listing Users</h1>

<table class="table">
    <%= for user <- @users do %>
    <tr>
        <% Logger.debug "Here is myTst: #{first_name user }" %>
        <td><%= render "user.html", user: user %></td>
        <td><%= link "View", to: user_path(@conn,:show, user.id) %></td>
    </tr>
    <% end %>
</table>
